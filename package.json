{
  "name": "react-material-cli-docs",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.12.1",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "axios": "^0.21.1",
    "date-fns": "^2.22.1",
    "husky": "^7.0.1",
    "i18next": "^20.3.2",
    "i18next-browser-languagedetector": "^6.1.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.11.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "typescript": "^4.1.2",
    "uuid": "^8.3.2",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test-coverage": "npm run test -- --coverage --runInBand --watchAll=false",
    "eject": "react-scripts eject",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.3.3",
    "@storybook/addon-essentials": "^6.3.3",
    "@storybook/addon-links": "^6.3.3",
    "@storybook/node-logger": "^6.3.3",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.3",
    "@types/enzyme": "^3.10.9",
    "@types/pretty": "^2.0.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/uuid": "^8.3.1",
    "enzyme": "^3.11.0",
    "pretty": "^2.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test-coverage"
    }
  },
  "resolutions": {
    "babel-loader": "8.1.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/src/components/**/*.{ts,tsx}",
      "!**/src/components/**/*.{stories,styles}.{ts,tsx}",
      "!**/src/components/**/index.{ts,tsx}",
      "!**/node_modules/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    }
  }
}
